{
  "name": "laser-calc-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:production": "NODE_ENV=production vite build --mode production",
    "build:staging": "NODE_ENV=staging tsc -b && vite build --mode staging",
    "build:analyze": "npm run build:production && npx vite-bundle-analyzer dist/stats.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "preview:production": "vite preview --mode production",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ci": "vitest run --coverage",
    "test:core": "vitest run src/__tests__/calculators/coreCalculators.test.ts",
    "test:core-framework": "vitest run src/__tests__/core/",
    "test:all-calculators": "tsx src/__tests__/runners/runAllTests.ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "deploy:staging": "npm run build:staging && vercel --prod",
    "deploy:production": "npm run build:production && vercel --prod",
    "deploy:cloudflare": "node deploy-cloudflare.js",
    "deploy:cf": "npm run build && npx wrangler pages deploy dist --project-name=laser-calc-app",
    "lighthouse": "lighthouse http://localhost:4173 --output=json --output-path=./lighthouse-report.json",
    "lighthouse:ci": "lhci autorun"
  },
  "dependencies": {
    "@tiptap/extension-image": "^3.3.1",
    "@tiptap/extension-link": "^3.3.1",
    "@tiptap/extension-table": "^3.3.1",
    "@tiptap/extension-table-cell": "^3.3.1",
    "@tiptap/extension-table-header": "^3.3.1",
    "@tiptap/extension-table-row": "^3.3.1",
    "@tiptap/react": "^3.3.1",
    "@tiptap/starter-kit": "^3.3.1",
    "chart.js": "^4.5.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "i18next": "^25.4.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "jspdf": "^3.0.2",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.542.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^15.7.3",
    "react-router-dom": "^7.8.2",
    "recharts": "^3.1.2",
    "wrangler": "^4.33.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.3.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "terser": "^5.43.1",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  }
}
